{
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": false,
  "endOfLine": "crlf",
  "trailingComma": "none",
  "plugins": [
    "prettier-plugin-astro",
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ]
}
